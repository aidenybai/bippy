{
  "name": "@bippy/shrinkwrap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:inject\" \"pnpm dev:next\" --kill-others --names \"INJECT,NEXT\" --prefix-colors \"blue,green\"",
    "dev:next": "next dev",
    "build": "pnpm build:inject && next build",
    "dev:inject": "tsup --watch",
    "build:inject": "tsup",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.14",
    "@sparticuz/chromium": "^123.0.0",
    "@sparticuz/chromium-min": "^123.0.0",
    "ai": "^4.1.41",
    "bippy": "^0.3.6",
    "css-to-tailwindcss": "^1.0.5",
    "extract-colors": "^4.1.1",
    "nanoid": "^5.1.0",
    "next": "15.1.7",
    "postcss": "^8.5.2",
    "puppeteer": "^22.6.2",
    "puppeteer-core": "^22.6.2",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "style-object-to-css-string": "^1.1.3",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "concurrently": "^9.1.2",
    "tsup": "^8.2.4",
    "typescript": "^5"
  }
}
